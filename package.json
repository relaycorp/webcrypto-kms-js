{
  "name": "@relaycorp/kms",
  "version": "1.0.0",
  "author": {
    "email": "no-reply@relaycorp.tech",
    "name": "Relaycorp, Inc.",
    "url": "https://relaycorp.tech/"
  },
  "description": "Client that abstracts Key Management Services (KMS) like GCP KMS",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/relaycorp/awala-keystore-gcp-js",
  "homepage": "https://docs.relaycorp.tech/awala-keystore-gcp-js/",
  "license": "MIT",
  "keywords": [
    "kms",
    "gcp",
    "gcp-kms"
  ],
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "static-checks": "run-p static-checks:*",
    "static-checks:lint": "tslint --project .",
    "static-checks:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test": "jest --coverage",
    "test:ci:unit": "run-s build test:ci:unit:jest",
    "test:ci:unit:jest": "jest --config jest.config.ci.js --coverage",
    "test:integration": "exit 0",
    "test:integration:local": "jest --config src/functional_tests/jest.config.js --runInBand --detectOpenHandles",
    "doc-api": "typedoc src/index.ts --out build/docs",
    "clean": "del-cli build test"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@relaycorp/shared-config": "^1.9.3",
    "@types/jest": "^28.1.6",
    "del-cli": "^5.0.0",
    "jest": "^28.1.3",
    "jest-extended": "^3.2.4",
    "mongodb-memory-server": "^8.11.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typedoc": "^0.23.25",
    "typescript": "^5.0.2"
  },
  "prettier": "@relaycorp/shared-config/.prettierrc.json",
  "publishConfig": {
    "access": "public"
  }
}
